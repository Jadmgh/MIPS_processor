	.rr2(rr2),
	.ValueSelect(ValueSelect)
);

// GClock
initial
begin
	repeat(2)
	begin
		GClock = 1'b0;
		GClock = #20000 1'b1;
		# 20000;
	end
	GClock = 1'b0;
end 

// GReset
initial
begin
	GReset = 1'b1;
	GReset = #20000 1'b0;
end 

// RAM_Clock
