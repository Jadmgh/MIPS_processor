module Lab2_vlg_sample_tst(
	GClock,
	GReset,
	RAM_Clock,
	ValueSelect,
	sampler_tx
);
input  GClock;
input  GReset;
input  RAM_Clock;
input [2:0] ValueSelect;
output sampler_tx;

reg sample;
time current_time;
always @(GClock or GReset or RAM_Clock or ValueSelect)
                                                                                
begin                                                 
 if ($realtime > 0)                                   
 begin                                                
	if ($realtime == 0 || $realtime != current_time)  
	begin									          
