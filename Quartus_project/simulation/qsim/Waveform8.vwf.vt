begin
	RAM_Clock = 1'b0;
	RAM_Clock = #25000 1'b1;
	# 5000;
	repeat(4)
	begin
		RAM_Clock = 1'b0;
		RAM_Clock = #5000 1'b1;
		# 5000;
	end
	RAM_Clock = 1'b0;
	RAM_Clock = #5000 1'b1;
	RAM_Clock = #5000 1'b0;
	RAM_Clock = #5000 1'b1;
	RAM_Clock = #5000 1'b0;
	RAM_Clock = #5000 1'b1;
	RAM_Clock = #4000 1'b0;
end 

